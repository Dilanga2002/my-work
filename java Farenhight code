public class Farenhight {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner ob = new Scanner(System.in);
        System.out.println("Enter Farenheight =");
        double farenheight = ob.nextDouble();
        
        double celsius = (farenheight - 32)*5/9;
        System.out.println("temp celsius ="+celsius);
        
        System.out.println();
         System.out.println("Enter width =");
         double width = ob.nextDouble();
          System.out.println("Enter length =");
        double length = ob.nextDouble();
        
        double area = width*length;
        
         System.out.println("area is  ="+area);
    }
    
