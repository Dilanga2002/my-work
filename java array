public class Arr {

    /**
     * @param args the command line arguments
     */
   public static void main(String[] args) {
       
       
       String [][] name = {{"name","kamal"},{"name","sunil"}};
       
       for(int i = 0; i<name.length; i++){
           for(int j = 0; j<name[i].length; j++){
               
               System.out.print(name[i][j]+ " ");
           }
             System.out.println();
       }
     
  }


}
